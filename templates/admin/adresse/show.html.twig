{% extends 'base.html.twig' %}

{% block title %}Adresse{% endblock %}

{% block body %}
    <div class="container">
        <h1>Adresse</h1>

        <table class="table">
            <tbody>
            <tr>
                <th>Id</th>
                <td>{{ adresse.id }}</td>
            </tr>
            <tr>
                <th>Label</th>
                <td>{{ adresse.label }}</td>
            </tr>
            <tr>
                <th>Number</th>
                <td>{{ adresse.number }}</td>
            </tr>
            <tr>
                <th>Street</th>
                <td>{{ adresse.street }}</td>
            </tr>
            <tr>
                <th>City</th>
                <td>{{ adresse.city }}</td>
            </tr>
            <tr>
                <th>Country</th>
                <td>{{ adresse.country }}</td>
            </tr>
            <tr>
                <th>Code Postal</th>
                <td>{{ adresse.codepostal }}</td>
            </tr>
            </tbody>
        </table>

        <a href="{{ path('admin_adresse_index') }}">back to list</a>

        <a href="{{ path('admin_adresse_edit', {'id': adresse.id}) }}">edit</a>

        {{ include('admin/adresse/_delete_form.html.twig') }}
    </div>
{% endblock %}
